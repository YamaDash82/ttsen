{
  "compilerOptions": {
    "noImplicityAny": true, 
    "module": "es2015", 
    "target": "es5", 
    "jsx": "react", 
    "lib": ["es2018", "dom"], 
    "moduleResolution": "node", 
    "removeComments": true, 
    "strict": true, 
    "noUnusedLocals": true, 
    "noUnusedParameters": false, 
    "noImplicitReturns": true, 
    "noFallthroughCasesInswitch": true, 
    "strictFunctionTypes": false
  }
}